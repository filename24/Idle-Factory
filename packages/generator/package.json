{
  "name": "@idle/generator",
  "scripts": {
    "build": "tsup",
    "lint": "prettier --check . \"!src/template\" && eslint src --ext .mjs,.js,.ts",
    "format": "prettier --write . \"!src/template\" && eslint src --ext .mjs,.js,.ts --fix"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "fs-extra": "^11.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11",
    "commander": "^10.0.0",
    "tsup": "^6.7.0"
  },
  "prettier": {}
}
