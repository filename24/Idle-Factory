{
  "name": "bot",
  "repository": "https://github.com/InfiniteTeam/Idle-Factory",
  "author": "__filename",
  "main": "build/index.js",
  "scripts": {
    "start": "yarn build && node .",
    "start:node": "node .",
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc && tsc-alias",
    "update": "yarn upgrade-interactive",
    "lint": "eslint . --ext .ts",
    "lint:fix": "prettier --write '**/*.ts' && eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@idle/database": "workspace:^",
    "@idle/utils": "workspace:^",
    "discord.js": "^14.11.0",
    "dokdo": "^0.6.2",
    "dotenv": "^16.0.3",
    "i18next": "^21.10.0",
    "node-fetch": "2.6.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "tsc-alias": "^1.8.5",
    "tsx": "^3.12.7"
  },
  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*",
    "eslintrc.*"
  ],
  "engines": {
    "node": ">=16.9"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js"
  },
  "packageManager": "yarn@3.2.2",
  "private": true
}
